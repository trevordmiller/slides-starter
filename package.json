{
  "name": "slides-some-title",
  "version": "1.0.0",
  "description": "Some description",
  "scripts": {
    "dev": "mdx-deck src/index.mdx",
    "precommit": "lint-staged",
    "format": "prettier --write '**/*.{js,json,md}'",
    "verify": "npm run lint && npm run build",
    "lint": "eslint .",
    "build": "npm run clean && npm run static && npm run html && npm run pdf",
    "clean": "rm -rf docs",
    "static": "mkdir -p docs/static && cp -R src/static docs/",
    "html": "mdx-deck build src/index.mdx -d docs",
    "pdf": "mdx-deck pdf src/index.mdx -d docs",
    "release": "git checkout master && git pull && npm run verify && git add -A && git commit -m 'Release' && git push"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "eslint": "^5.4.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "mdx-deck": "^1.6.9",
    "prettier": "^1.14.2"
  },
  "dependencies": {
    "mdx-deck-code-surfer": "^0.1.2",
    "nova-colors": "^2.1.5",
    "raw-loader": "^0.5.1",
    "react-syntax-highlighter": "^8.0.1",
    "styled-components": "^3.4.5"
  },
  "engines": {
    "node": "8.x.x",
    "npm": "6.x.x"
  }
}
